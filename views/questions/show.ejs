<h1><%= question.question %></h1>
<h4> - <%= question.option1 %></h3>
<h4> - <%= question.option2 %></h3>

<form method="get" action="/questions/<%= question._id %>/edit">
    <button type="submit" class="btn-large waves-effect waves-light deep-purple lighten-2">Edit</button>
</form>
<form method="post" action="/questions/<%= question.id %>">
  <input type="hidden" name="_method" value="delete">
  <button type="submit" class="btn-large waves-effect waves-light red">Delete</button>
</form>
